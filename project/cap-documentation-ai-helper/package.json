{
  "name": "cap-ai-vector-engine-example",
  "version": "1.0.0",
  "description": "A CAP & AI Example using the CAP LLM Plugin.",
  "repository": "https://github.tools.sap/D061070/cap-ai-vector-engine-example.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.14.0",
    "@sap/cds": "8.1.0",
    "@sap/cds-hana": "^2",
    "cap-llm-plugin": "^1.3.1",
    "langchain": "^0.1.36",
    "cors": "^2.8.5",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "1.7.3",
    "@sap/cds-dk": "8.0.2"
  },
  "scripts": {
    "build": "cds build",
    "build_sqlite": "cds deploy --profile sqlite",
    "hana": "cds deploy --to hana:cap-ai-codejam-hdb --auto-undeploy",
    "start": "cds-serve",
    "watch": "cds watch --profile hybrid --livereload false",
    "sqlite": "cds watch --profile sqlite --livereload false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "hana"
      },
      "auth": "dummy",
      "destinations": true,
      "[hybrid]": {
        "db": "hana"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
